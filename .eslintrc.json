{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true
  },
  "ignorePatterns": [
    "dist",
    "out",
    "node_modules"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
}
